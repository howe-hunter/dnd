	<element name="-.01 Weight" type="Item" source="Internal" id="ID_INTERNAL_ITEM_MINUS_POINT_ZERO_ONE_WEIGHT">
		<description>
			<p>Subtracts .01 from inventory weight. Item is stackable.</p>
		</description>
		<setters>
			<set name="weight" lb="-.01">-.01 lb.</set>
			<set name="category">Additional Feature</set>
			<set name="inventory-hidden">true</set>
			<set name="stackable">true</set>
		</setters>
	</element>

